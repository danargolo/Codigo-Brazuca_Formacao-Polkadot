programa {
  funcao inicio() {
    real num1, num2, num3, avg

    escreva("Digite o primeiro número:")
    leia(num1)

    escreva("Digite o segundo número:")
    leia(num2)

    escreva("Digite o terceiro número:")
    leia(num3)

    avg = (num1 + num2 + num3) / 3

    escreva("A média dos números é:", avg)
  }
}
